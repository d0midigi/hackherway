<!-- Typewriter Animation -->
<script>
var TxtType = function(el, toRotate, period) {
  this.toRotate = toRotate;
  this.el = el;
  this.loopNum = 0;
  this.period = parseInt(period, 10) || 2000;
  this.txt = '';
  this.isDeleting = false;
  this.tick();
};
TxtType.prototype.tick = function() {
  var i = this.loopNum % this.toRotate.length;
  var fullTxt = this.toRotate[i];
  var wrapEl = this.el.querySelector('.wrap');
  var typingNow = false;
  if (this.isDeleting) {
    this.txt = fullTxt.substring(0, this.txt.length - 1);
    typingNow = true;
  } else if (this.txt !== fullTxt) {
    this.txt = fullTxt.substring(0, this.txt.length + 1);
    typingNow = true;
  }
    } else if (this.txt !== fullTxt) {
    this.txt = fullTxt.substring(0, this.txt.length + 1);
    typingNow = true;
  }
  wrapEl.innerHTML = this.txt;
  if (typingNow) {
    wrapEl.classList.add('flashing-border');
  } else {
    wrapEl.classList.remove('flashing-border');
  }
  var that = this;
  var delta = 80 - Math.random() * 40;
  if (this.isDeleting) { delta /= 2; }
  if (!this.isDeleting && this.txt === fullTxt) {
    wrapEl.classList.remove('flashing-border');
    delta = this.period;
    this.isDeleting = true;
  } else if (this.isDeleting && this.txt === '') {
    this.isDeleting = false;
    this.loopNum++;
    delta = 350;
  }
  setTimeout(function() { that.tick(); }, delta);
};
document.addEventListener("DOMContentLoaded", function() {
  var elements = document.getElementsByClassName('typewrite');
  for (var i=0; i<elements.length; i++) {
    var toRotate = elements[i].getAttribute('data-type');
    var period = elements[i].getAttribute('data-period');
    if (toRotate) {
      new TxtType(elements[i], JSON.parse(toRotate), period);
    }
  }
});
</script>

<!-- SIDEBAR DROPDOWN (Accordion, only one expands) -->
<script>
document.addEventListener('DOMContentLoaded', function () {
  // Only one sidebar dropdown expands at a time (accordion mode)
  document.querySelectorAll('.idocs-navigation .has-children > .nav-link').forEach(function(link) {
    link.addEventListener('click', function (e) {
      e.preventDefault();
      var parent = link.parentElement;
      // Accordion: collapse others
      document.querySelectorAll('.idocs-navigation .has-children.expanded').forEach(function(item) {
        if (item !== parent) item.classList.remove('expanded');
      });
      parent.classList.toggle('expanded');
    });
  });
});
</script>

<!-- MAIN NAVBAR NESTED DROPDOWN (Bootstrap 4 fix for nested) -->
<script>
document.addEventListener('DOMContentLoaded', function () {
  // Fix for Bootstrap 4 nested dropdowns
  document.querySelectorAll('.dropdown-submenu .dropdown-toggle').forEach(function(element) {
    element.addEventListener('click', function(e) {
      e.stopPropagation();
      e.preventDefault();
      var submenu = element.nextElementSibling;
      if (submenu && submenu.classList.contains('dropdown-menu')) {
        submenu.classList.toggle('show');
        // Close other open submenus at same level
        var siblings = element.parentElement.parentElement.querySelectorAll('.dropdown-menu.show');
        siblings.forEach(function(sib) {
          if (sib !== submenu) sib.classList.remove('show');
        });
      }
    });
  });
  // On click outside, close all open submenus
  document.addEventListener('click', function() {
    document.querySelectorAll('.dropdown-menu.show').forEach(function(menu){
      menu.classList.remove('show');
    });
  });
});
</script>
